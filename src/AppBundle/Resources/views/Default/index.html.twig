                    </div>
                    <!-- ./panel-collapse -->
                </div>
                <!-- .panel -->

                <div class="panel">
                    <div class="panel-heading">
                        <h4 class="panel-title">
                            <a href="#faq-general-2" data-parent="#accordion-help" data-toggle="collapse"
                               class="accordion-toggle"><i class="fa accordion-caret"></i>
                                Cantidad de Empleados aportantes </a>
