<fieldset>
    <legend>Datos Personales</legend>
    {{ form_row(form['cuil']) }}
    {{ form_row(form['nombre']) }}
    {{ form_row(form['sexo']) }}
    {{ form_row(form['estado_civil']) }}

    <legend>Datos de Localizaci√≥n</legend>
    {{ form_row(form['telefono']) }}
    {{ form_row(form['email']) }}
    {{ form_row(form['direccion']) }}
    {{ form_row(form['localidad']) }}
    <legend>Datos Laborales</legend>
    {{ form_row(form['legajo']) }}
    <div class="form-group">
        {{ form_label(form['fecha_ingreso']) }}
        <div class="col-sm-10 input-group date">
            {{ form_widget(form['fecha_ingreso']) }}
            {#<div class="input-group-addon">.00</div>#}
            <span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
        </div>
    </div>

    <div class="form-group">
        {{ form_label(form['fecha_baja']) }}
        <div class="col-sm-10 input-group date">
            {{ form_widget(form['fecha_baja']) }}
            <span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
        </div>
    </div>

</fieldset>

