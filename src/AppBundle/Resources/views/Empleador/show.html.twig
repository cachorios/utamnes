{% extends ['::layout_backend.html.twig','layout.html.twig'] %}

    {% block contenido -%}
    {% embed '::portlet.html.twig' with {'titulo': 'Ver Empleador: ' ~ entity} %}
        {% block in_header %} 
        <div class='pull-right'>
        <div class="row">
                <div class="col-md-1 ">
        <div class="pull-right">
            {{ form(delete_form) }}
        </div>
    </div>
    <a href="{{ path('admin_empleador_edit', { 'id': entity.id }) }}"
       class="btn btn-sm btn-default">
        Editar
    </a>





<a href="{{ path('admin_empleador') }}" class="btn btn-sm btn-default">
    Ir a la lista
</a>


        </div>
    </div>
        {% endblock %}
            {% block cuerpo %} 
    <p></p>

    <table class="table">
        <tbody>
            <tr>
                <th class="show-header">Id</th>
                    <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th class="show-header">Razon</th>
                    <td>{{ entity.razon }}</td>
            </tr>
            <tr>
                <th class="show-header">Cuit</th>
                    <td>{{ entity.cuit }}</td>
            </tr>
            <tr>
                <th class="show-header">Direccion</th>
                    <td>{{ entity.direccion }}</td>
            </tr>
            <tr>
                <th class="show-header">Telefono</th>
                    <td>{{ entity.telefono }}</td>
            </tr>
            <tr>
                <th class="show-header">Email</th>
                    <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th class="show-header">Localidad</th>
                    <td>{{ entity.localidad }}</td>
            </tr>
            <tr>
                {#<th class="show-header">Fecha_actualizacion</th>#}
                    {#<td>{{ entity.fechaactualizacion|date('Y-m-d H:i:s') }}</td>#}
            </tr>
        </tbody>
    </table>


    {% endblock %} 
    {% endembed %} 
    {% endblock contenido %}
